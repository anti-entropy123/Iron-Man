
.map{
	height:100vh
}
.changebtn{
	position:absolute;
	top:200rpx;
	left:50rpx;
	width:200rpx;
	height:50rpx;
	border-radius: 25rpx;
	background: #ffffff;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-shadow: 0 5rpx 5rpx 2rpx #e6e6e6;
	        box-shadow: 0 5rpx 5rpx 2rpx #e6e6e6;
}
.selectedbtn{
	width:50%;
	background: #10518e;
	color:#ffffff;
	font-size:24rpx;
	line-height:50rpx;
	text-align:center;
}
.unselectedbtn{
	width:50%;
	background: #ffffff;
	color:#000000;
	font-size:24rpx;
	line-height:50rpx;
	text-align:center;
}


